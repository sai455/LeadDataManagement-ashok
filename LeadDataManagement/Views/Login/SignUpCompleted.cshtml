@model LeadDataManagement.Models.ViewModels.UserViewModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <title>SNK Scrub Tool | SignUp</title>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />

    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Quicksand:500,700" rel="stylesheet">

    <link rel="stylesheet" type="text/css" href="~/Content/lead-app-content/css/bootstrap.min.css">

    <link rel="stylesheet" href="~/Content/lead-app-content/css/waves.min.css" type="text/css" media="all">
    <link rel="stylesheet" type="text/css" href="~/Content/lead-app-content/css/feather.css">

    <link rel="stylesheet" type="text/css" href="~/Content/lead-app-content/css/themify-icons.css">

    <link rel="stylesheet" type="text/css" href="~/Content/lead-app-content/css/icofont.css">

    <link rel="stylesheet" type="text/css" href="~/Content/lead-app-content/css/font-awesome.min.css">

    <link rel="stylesheet" type="text/css" href="~/Content/lead-app-content/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/Content/lead-app-content/css/pages.css">

</head>
<body themebg-pattern="theme1">

    <div class="theme-loader">
        <div class="loader-track">
            <div class="preloader-wrapper">
                <div class="spinner-layer spinner-blue">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="gap-patch">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
                <div class="spinner-layer spinner-red">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="gap-patch">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
                <div class="spinner-layer spinner-yellow">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="gap-patch">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
                <div class="spinner-layer spinner-green">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="gap-patch">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <section class="login-block">

        <div class="container-fluid">
            <div class="row">
                <div class="col-sm-12">

                    <form class="md-float-material form-material" autocomplete="off">
                        <div class="auth-box card">
                            <div class="card-block">
                                <div class="row m-b-20">
                                    <div class="col-md-12">
                                        <h3 class="text-center txt-primary">Sign up</h3>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-10">
                                        <p class="text-inverse text-left m-b-0 text-success">Registration completed successfully,<br /> However admin approval is required. </p>
                                        <p class="text-inverse text-left"><a class="text-bold" href="@Url.Action("Index","Login")">Continue to Login</a></p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>

                </div>

            </div>

        </div>

    </section>

    <script type="text/javascript" src="~/Content/lead-app-content/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/Content/lead-app-content/js/jquery-ui.min.js"></script>
    <script type="text/javascript" src="~/Content/lead-app-content/js/popper.min.js"></script>
    <script type="text/javascript" src="~/Content/lead-app-content/js/bootstrap.min.js"></script>

    <script src="~/Content/lead-app-content/js/waves.min.js" type="text/javascript"></script>

    <script type="text/javascript" src="~/Content/lead-app-content/js/jquery.slimscroll.js"></script>

    <script type="text/javascript" src="~/Content/lead-app-content/js/modernizr.js"></script>
    <script type="text/javascript" src="~/Content/lead-app-content/js/css-scrollbars.js"></script>

    <script type="text/javascript" src="~/Content/lead-app-content/js/i18next.min.js"></script>
    <script type="text/javascript" src="~/Content/lead-app-content/js/i18nextxhrbackend.min.js"></script>
    <script type="text/javascript" src="~/Content/lead-app-content/js/i18nextbrowserlanguagedetector.min.js"></script>
    <script type="text/javascript" src="~/Content/lead-app-content/js/jquery-i18next.min.js"></script>
    <script type="text/javascript" src="~/Content/lead-app-content/js/common-pages.js"></script>
    <script src="~/Content/lead-app-content/js/rocket-loader.min.js" data-cf-settings="a371f3c3996957007cacd73a-|49" defer=""></script>

</body>
</html>
